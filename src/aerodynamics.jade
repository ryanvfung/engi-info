- var subtitle = 'Aerodynamics';
- var home = '.';


doctype html
html(lang='en')
  include ../inc/head.jade
  body
    include ../inc/header.jade
    div#main-wrapper: main(role='main')
      include ../inc/latex-declarations.jade



      section
        h1#definitions Definitions
        table.c-quantity-table
          thead
            tr
              th Symbol
              th Quantity
              th Units
          tbody
            tr
              td: var C#[sub D]
              td Drag coefficient
              td
            tr
              td: var c#[sub D]
              td Sectional drag coefficient
              td
            tr
              td: var C#[sub L]
              td Lift coefficient
              td
            tr
              td: var c#[sub L]
              td Sectional lift coefficient
              td
            tr
              td: var D
              td Drag
              td N
            tr
              td: var L
              td Lift
              td N
            tr
              td: var M
              td Mach number
              td
            tr
              td: var &alpha;
              td Angle of Attack
              td rad, &deg;



      section
        h1#flow-regimes Flow regimes
        table
          tbody
            tr
              td #[var M] < 0.3
              td Effectively Incompressible
            tr
              td #[var M] < 1
              td Subsonic
            tr
              td #[var M] > 1
              td Supersonic
            tr
              td 0.8 < #[var M] < 1.2
              td Transonic
            tr
              td #[var M] > 5
              td Hypersonic



      section
        h1#vorticity Vorticity
        p
          | \[
          |   \omega \quad = \quad \nabla \times \mathbf{u} \quad = \quad
          |   \left| \begin{matrix}
          |     i & j & k \\
          |     \pddx{}{x} & \pddx{}{y} & \pddx{}{z} \\
          |     u & v & w \\
          |   \end{matrix} \right|
          |   \quad = \quad \frac{1}{r}
          |   \left| \begin{matrix}
          |     \mathbf{e}_r & r\mathbf{e}_\theta  & \mathbf{e}_z \\
          |     \pddx{}{r} & \pddx{}{\theta} & \pddx{}{z} \\
          |     v_r & rv_\theta & v_z \\
          |   \end{matrix} \right|
          | \]

    include ../inc/footer.jade
