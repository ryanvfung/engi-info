      section
        h1#vorticity Vorticity
        p
          | \[
          |   \omega \quad = \quad \nabla \times \mathbf{u} \quad = \quad
          |   \left| \begin{matrix}
          |     i & j & k \\
          |     \pddx{}{x} & \pddx{}{y} & \pddx{}{z} \\
          |     u & v & w \\
          |   \end{matrix} \right|
          |   \quad = \quad \frac{1}{r}
          |   \left| \begin{matrix}
          |     \mathbf{e}_r & r\mathbf{e}_\theta  & \mathbf{e}_z \\
          |     \pddx{}{r} & \pddx{}{\theta} & \pddx{}{z} \\
          |     v_r & rv_\theta & v_z \\
          |   \end{matrix} \right|
          | \]



      section
        h1#supersonic-flow Supersonic flow

        h2#prandtl-meyer-function Prandtl-Meyer function
        p
          | \[ \nu(M) =
          |   \sqrt{\frac{\gamma+1}{\gamma-1}} \cdot
          |   \tan^{-1}{\left(\sqrt{\frac{\gamma-1}{\gamma+1}(M^2 - 1)}\right)}
          |   - \tan^{-1}{\left(\sqrt{M^2 - 1}\right)}
          | \]

        h2#shock-jump-relations Shock jump relations
        p \[ \frac{\rho_2}{\rho_1} = \frac{M_1^2(\gamma+1)}{2+(\gamma-1)M_1^2} \]
        p \[ \frac{p_2}{p_1}=1+\frac{2\gamma(M_1^2-1)}{\gamma+1} \]
        p \[ M_2^2=\frac{2+(\gamma-1)M_1^2}{2\gamma M_1^2-(\gamma-1)} \]



